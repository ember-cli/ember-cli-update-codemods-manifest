    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prefer-let": "^1.1.0",
    "fs-extra": "^10.0.0",
    "git-fixtures": "^4.0.0",
    "mocha": "^9.1.2",
    "mocha-helpers": "^6.0.1",
    "monorepo-next": "^7.0.0",
    "renovate-config-standard": "2.1.2",
    "standard-node-template": "2.0.0"
