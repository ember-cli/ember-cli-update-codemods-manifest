  "engines": {
    "node": ">=12.13"
  },
  "devDependencies": {
    "@crowdstrike/commitlint": "^4.0.0",
    "@kellyselden/node-template": "2.2.0",
    "chai": "^4.3.4",
    "ember-addon-codemods-manifest": "",
    "ember-app-codemods-manifest": "",
    "ember-cli-update": "1.0.1",
    "eslint": "^8.0.0",
    "eslint-config-sane": "^1.0.0",
    "eslint-config-sane-node": "^1.1.0",
